{
    "manifest_version": 3,
    "name": "SMART-IES",
    "version": "1.0",
    "description": "SMART-IES",

    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": [
            "js/common.js"
        ],
        "css": [
            "css/style.css"
        ],
        "run_at": "document_end"
    }],
    "permissions": [
        "activeTab",
        "tabs",
        "storage"
    ],
    "action": {
        "default_popup": "popup/popup.html"
    },
    "host_permissions": [
        "<all_urls>"
    ]
}